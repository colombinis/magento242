<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="customer_address_head_components" template="Magento_Customer::js/components.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <!-- <block class="Magento\Customer\Block\Address\Grid" name="address_grid" template="Magento_Customer::address/grid.phtml" cacheable="false"/> -->
            <block class="Magento\Framework\View\Element\Template" name="newtab_grid" template="Sacsi_Newtabcustomer::newtab/grid.phtml" cacheable="false">
                <arguments>
                    <argument name="view_model_tab1" xsi:type="object">Sacsi\Newtabcustomer\ViewModel\Productos</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
